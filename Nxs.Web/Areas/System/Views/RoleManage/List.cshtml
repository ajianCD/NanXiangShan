@model List<Nxs.Data.AspNetRoles>
@{
    Layout = null;
}
<table class="table table-bordered">
        <thead>
            <tr>
                <td width="10%">序号</td>
                <td width="20%">用户名</td>
                <td width="40%">描述</td>
                <td width="10%">状态</td>
                <td width="20%">操作</td>
            </tr>
        </thead>
        @for (int i = 0; i < Model.Count; i++)
        {
            var item = Model[i];
            <tr>
                <td>@i</td>
                <td>@item.Name</td>
                <td>@item.Discriminator</td>
                <td>@(item.State == 1 ? "可用" : "禁用")</td>
                <td>
                    <button type="button" class="btn btn-primary btn-sm">禁用</button>
                    <button type="button" class="btn btn-danger btn-sm">删除</button>
                </td>
            </tr>


        }
    </table>